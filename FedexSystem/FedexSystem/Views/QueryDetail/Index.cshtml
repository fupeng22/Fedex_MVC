@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/JqueryUI/themes/default/easyui.css")"  rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script  src="@Url.Content("~/JS/QueryDetail.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBar" style="padding: 2px; height: auto">
        <div class="easyui-panel" title="查询" style="height: 58px; padding: 3px" collapsible="true">
            岗位:<select id="ddlPostion" class="easyui-combotree" style="width: 200px;" panelheight="auto">
            </select>&nbsp;X光机扫描时间段:<input type="text" id="inputBeginDate" name="inputBeginDate" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'});"
                class="Wdate" style="width: 160px"  value='@(DateTime.Now.ToString("yyyy-MM-dd") + " 00:00:00")'/>---<input type="text" id="inputEndDate" name="inputEndDate"
                    onfocus="WdatePicker({dateFmt:'yyyy-MM-dd HH:mm:ss'});" class="Wdate" style="width: 160px"  value='@(DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss"))'/>
            <a href="#" id="btnQuery" name="btnQuery" class="easyui-linkbutton" data-options="iconCls:'icon-search'">
                查询</a> <a href="#" id="btnReset" name="btnReset" class="easyui-linkbutton" data-options="iconCls:'icon-undo'">
                    重置</a>&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" id="menuButtonReport" class="easyui-menubutton"
                        data-options="menu:'#menuButtons',iconCls:'icon-report',plain:false">报表</a>
            <div id="menuButtons" style="width: 150px;">
                <div id="btnPrint" name="btnPrint" data-options="iconCls:'icon-print'">
                    打印(PDF)</div>
                <div id="btnExcel" name="btnExcel" data-options="iconCls:'icon-excel'">
                    导出(Excel)</div>
            </div>
        </div>
        <div class="easyui-panel" title="查询结果" style="height: 20px; padding: 1px" collapsible="false">
            <span style="color: Blue; font-weight: bold">岗位:</span><span id="lblPostion" style="width: 300px;
                color: Red; font-weight: bold"></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span style="color: Blue; font-weight: bold">
                    起始时间:</span><span id="lblStartTime" style="width: 150px; color: Red; font-weight: bold"></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span
                        style="color: Blue; font-weight: bold">结束时间:</span><span id="lblEndTime" style="width: 150px;
                            color: Red; font-weight: bold"></span>
        </div>
    </div>
    <div id="DG_XRayScanResult" fit="true" style="width: 800px; height: 600px">
    </div>
</body>
</html>
