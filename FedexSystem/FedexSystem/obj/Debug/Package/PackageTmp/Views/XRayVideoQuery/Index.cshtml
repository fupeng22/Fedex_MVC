@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Index</title>
    <script src="@Url.Content("~/Scripts/jquery-1.6.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/jquery.easyui.min.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/JqueryUI/themes/default/easyui.css")"  rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/JqueryUI/themes/icon.css")"  rel="stylesheet" type="text/css" />
    <script  src="@Url.Content("~/JS/videoPlay.js")" type="text/javascript"></script>
    <script  src="@Url.Content("~/JS/XRayVideoQuery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JqueryUI/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/My97DatePicker/WdatePicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/JS/Common/Common.js")" type="text/javascript"></script>
</head>
<body>
    <div id="toolBar" style="padding: 2px; height: auto">
        <div class="easyui-panel" title="普通查询" style="height: 58px; padding: 3px" collapsible="true">
            <input type="hidden" id="hidSearchType" name="hidSearchType" value="-1" />
            <input type="hidden" id="hidCameraInfo" name="hidCameraInfo" value='@ViewData["hidCameraInfo"]' />
            货物条码:<input type="text" id="txtCode" name="txtCode" />&nbsp;岗位识别码:<input type="text"
                id="txtCP" name="txtCP" />
            <a href="#" id="btnQuery" name="btnQuery" class="easyui-linkbutton" data-options="iconCls:'icon-search'">
                查询</a> <a href="#" id="btnReset" name="btnReset" class="easyui-linkbutton" data-options="iconCls:'icon-undo'">
                    重置</a>&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" id="menuButtonReport" class="easyui-menubutton"
                        data-options="menu:'#menuButtons',iconCls:'icon-report',plain:false">报表</a>
            <div id="menuButtons" style="width: 150px;">
                <div id="btnPrint" name="btnPrint" data-options="iconCls:'icon-print'">
                    打印(PDF)</div>
                <div id="btnExcel" name="btnExcel" data-options="iconCls:'icon-excel'">
                    导出(Excel)</div>
            </div>
        </div>
        <div class="easyui-panel" title="高级查询" style="height: 58px; padding: 3px" collapsible="true">
            X光机扫描时间段:<input type="text" id="inputBeginDate" name="inputBeginDate" onfocus="WdatePicker();"
                class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>---<input type="text" id="inputEndDate" name="inputEndDate"
                    onfocus="WdatePicker();" class="Wdate" style="width: 90px"  value='@(DateTime.Now.ToString("yyyy-MM-dd"))'/>&nbsp;&nbsp;货物条码:<input
                        type="text" id="txtGCode" name="txtGCode" />&nbsp;&nbsp;扫描操作员:<input type="text"
                            id="txtGOperator" name="txtGOperator" />
            <a href="#" id="GbtnQuery" name="GbtnQuery" class="easyui-linkbutton" data-options="iconCls:'icon-search'">
                查询</a> <a href="#" id="GbtnReset" name="GbtnReset" class="easyui-linkbutton" data-options="iconCls:'icon-undo'">
                    重置</a>
        </div>
    </div>
    <div id="DG_XRayScanResult" fit="true" style="width: 800px; height: 600px">
    </div>
    <div id="dlg_VideoRePlay" data-options="iconCls:'icon-save'" style="padding: 0px;
        width: 400px; height: 300px;">
    </div>
</body>
</html>
